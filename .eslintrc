{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-console": 1,
    "no-cond-assign": 1,
    "no-unused-vars": 1,
    "no-undef": 1
  }
}
